{% set title = doctitle + ' | DataStation' %}

{% extends 'layout.tmpl' %}

{% block body %}
<section>
  <div class="container">
    <h1>{{doctitle}}</h1>
    {% if subtitle %}{{ subtitle }}{% endif %}
</section>
<section class="post-body">
  <div class="container">
    <p class="what-is-datastation"><label>What is DataStation?</label><small>{{ description }}</small></p>
    {% block docbody %}{% endblock %}
  </div>
</section>
{% endblock %}